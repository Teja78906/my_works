{
    "docs": [
        "Converts between REWA and a wrapped REWA DCDT token.",
        "\\t1 REWA = 1 wrapped REWA and is interchangeable at all times.",
        "Also manages the supply of wrapped REWA tokens."
    ],
    "name": "RewaDcdtSwap",
    "endpoints": [
        {
            "name": "issueWrappedRewa",
            "payableInTokens": [
                "REWA"
            ],
            "inputs": [
                {
                    "name": "token_display_name",
                    "type": "bytes"
                },
                {
                    "name": "token_ticker",
                    "type": "bytes"
                },
                {
                    "name": "initial_supply",
                    "type": "BigUint"
                }
            ],
            "outputs": []
        },
        {
            "name": "mintWrappedRewa",
            "inputs": [
                {
                    "name": "amount",
                    "type": "BigUint"
                }
            ],
            "outputs": []
        },
        {
            "name": "wrapRewa",
            "payableInTokens": [
                "REWA"
            ],
            "inputs": [],
            "outputs": []
        },
        {
            "name": "unwrapRewa",
            "payableInTokens": [
                "*"
            ],
            "inputs": [],
            "outputs": []
        },
        {
            "name": "getLockedRewaBalance",
            "inputs": [],
            "outputs": [
                {
                    "type": "BigUint"
                }
            ]
        },
        {
            "name": "getWrappedRewaTokenIdentifier",
            "inputs": [],
            "outputs": [
                {
                    "type": "TokenIdentifier"
                }
            ]
        },
        {
            "name": "getUnusedWrappedRewa",
            "inputs": [],
            "outputs": [
                {
                    "type": "BigUint"
                }
            ]
        }
    ],
    "types": {}
}
